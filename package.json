    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.4",
    "prism-react-renderer": "^2.4.1",
